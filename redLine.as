package {		import flash.display.MovieClip;	import flash.events.*;		public class redLine extends MovieClip {						public function redLine() {			this.addEventListener(Event.ENTER_FRAME,checkFrame);			//(thescoretextfield.score as TextField).embedFonts = true;			//thescoretextformat.font = Config.getInstance().timetableFont.fontName;			//thescoretextfield.score.text = score;			//(thescoretextfield.score as TextField).setTextFormat(thescoretextformat);		}				public function checkFrame(e:Event) {			if(this.currentFrame == 11) {				this.stop();				this.parent.removeChild(this);				this.removeEventListener(Event.ENTER_FRAME,checkFrame);			}		}			}		}